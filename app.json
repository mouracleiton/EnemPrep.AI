{
  "expo": {
    "name": "ENEM Prep AI",
    "slug": "enem-prep-ai",
    "version": "0.0.1-alpha",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "enemprepai",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3498db"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/img/*",
      "assets/images/*",
      "assets/enem_data_with_lessons.json"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enemprepai",
      "infoPlist": {
        "UIBackgroundModes": ["fetch"],
        "NSPhotoLibraryUsageDescription": "Este aplicativo não precisa de acesso à sua biblioteca de fotos."
      },
      "jsEngine": "hermes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3498db"
      },
      "package": "com.enemprepai",
      "permissions": [],
      "versionCode": 1,
      "jsEngine": "hermes"
    },
    "plugins": [
      "expo-file-system"
    ],
    "extra": {
      "eas": {
        "projectId": "enem-prep-ai"
      }
    }
  }
}
